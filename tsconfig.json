{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "outDir": "./build/dev",
    "rootDir": "./",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/*", "./test/*"]
}
